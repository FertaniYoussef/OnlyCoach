            )

            // on star hover, change the current and the previous stars to the active color
            const stars = document.querySelectorAll('.star')
            const starContainer = document.getElementById('star-container')
            const starColor = '#FBBF24'
            const starColorInactive = '#E5E7EB'

            console.log(stars)
            
            // loop over the starts and add event listeners to them
            stars.forEach(star => {
                star.addEventListener('mouseover', () => {
                    const starId = star.id
                    const starNumber = starId.match(/\d+/)[0]
                    const starsToChange = document.querySelectorAll(`.star:nth-child(-n+${starNumber})`)
                    starsToChange.forEach(star => {
                        star.style.fill = starColor
                    })
                })
                 star.addEventListener('click', () => {
                    const starId = star.id
                    const starNumber = starId.match(/\d+/)[0]
                    const ratingInput = document.getElementById('rating')
                    ratingInput.value = starNumber
                    // form submit
                    const form = document.getElementById('rating-form')
                    form.submit()
                })
            })

            // on star mouse leave, change the current and the previous stars to the inactive color
            stars.forEach(star => {
                star.addEventListener('mouseleave', () => {
                    const starId = star.id
                    const starNumber = starId.match(/\d+/)[0]
                    const starsToChange = document.querySelectorAll(`.star:nth-child(-n+${starNumber})`)
                    starsToChange.forEach(star => {
                        star.style.fill = starColorInactive
                    })
                })
            })


            


            



           
           
           









            





