            var slug = url => new URL(url).pathname.match(/[^\/]+/g)
            const id = slug(window.location.href)
               async function getData() {
                const response = await fetch('/coach/dashboard/api/course/'+id[1], {
             method: 'POST',
                headers: {
                 'Content-Type': 'application/json',
                'Accept': 'application/json'
             },
            })
             .then(res => res.json())
             .catch(err => console.log(err))

             return response


            }

            // call the function then group sections and resources by section id
           const data = getData().then(data => {
                // extract section and group it with its resources by section id
                const sections = data.sections.map(section => {
                    const resources = data.resources.filter(resource => resource.id === section.id)
                    return { ...section, resources }
                })
                console.log(sections);
                // append sections to the dom
                const sectionContainer = document.getElementById('sections')
                
                /* WIP */
                var n = 1
                sections.forEach(section => {
                    var link = /[^/]*$/.exec(section.resources[0].link)[0];
                    link = link.replace("watch?v=", "");
                    console.log(link)
                    const div = document.createElement('div');
                    const sectionId = `section${section.id}`;
                    div.id = sectionId;
                    div.innerHTML = `<div id="${sectionId}">
                    <div class="bg-black w-full py-4 mb-4 shadow-md flex justify-between items-center px-4 rounded-md">
                    <h1 class="text-lg text-white font-medium">Section ${n} : ${section.title}</h1>
                    <span class="text-sm text-sky-200">${n}/${sections.length}</span></div>
                    <iframe class="w-full h-[25rem]" src="https://www.youtube.com/embed/${link}" title="YouTube video player" frameborder="0" allow="accelerometer; autoplay; clipboard-write; encrypted-media; gyroscope; picture-in-picture" allowfullscreen></iframe>

                    <h1 class="text-lg text-gray-800 font-medium mt-4">Description</h1>
                    <p class="text-sm text-gray-500">${section.resources[0].description}</p>

                    </div>`


                    sectionContainer.appendChild(div)
                    n++
                })
    }

